{
  "family": "my-nodejs-app-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "my-nodejs-app-container",
      "image": "your-docker-image-repository-url/my-nodejs-app:latest",
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "0.5GB",
  "executionRoleArn": "arn:aws:iam::889849722820:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::889849722820:role/aws-service-role/ecs.amazonaws.com/AWSServiceRoleForECS",
  "volumes": [],
  "placementConstraints": [],
  "proxyConfiguration": null
}
